<li class="linklist-item
  {% if link.active %}link-active{% endif %}
  {% if link.child_active %}link-child-active{% endif %}">
  <a href="{{ link.url }}" class="linklist-link linklist-link-{{ link.handle }}">{{ link.title }}</a>
  {% if link.levels > 0 %}
    {% content_for "block", type: "linklist", id: "test2" %}
  {% endif %}
</li>

{% schema %}
{
  "name": "Linklist Link",
  "tag": null,
  "settings": [

  ],
  "presets": [

  ]
}
{% endschema %}
{% comment %} {
  "type": "link_list",
  "id": "linklist",
  "label": "Link list",
  "info": "Select a link list to display"
},
{
  "type": "checkbox",
  "id": "show_title",
  "label": "Show linklist title",
  "default": true
} {% endcomment %}